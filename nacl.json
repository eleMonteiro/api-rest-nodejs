[
  {
    "group": "ADMIN",
    "permissions": [
      {
        "resource": "*",
        "methods": "*",
        "action": "allow"
      }
    ]
  },
  {
    "group": "CLIENTE",
    "permissions": [
      {
        "resource": "demands/*",
        "methods": [
          "POST",
          "GET"
        ],
        "action": "allow"
      },
      {
        "resource": "itens/*",
        "methods": [
          "GET"
        ],
        "action": "allow"
      },
      {
        "resource": "users/:id",
        "methods": [
          "GET",
          "PUT"
        ],
        "action": "allow"
      },
      {
        "resource": "dishes/*",
        "methods": [
          "GET"
        ],
        "action": "allow"
      },
      {
        "resource": "addresses/*",
        "methods": [
          "GET",
          "POST"
        ],
        "action": "allow"
      }
    ]
  }
]